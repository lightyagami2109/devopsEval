pipeline{
    agentany

    tools{
        maven 'maven'
    }

    stages{

        stage('build'){
            steps{
                     sh 'mvn -B -DskipTests clean package'
            }
        }

        stage('test'){
            steps{
                 sh 'mvn test'
            }
        }

        stage('deliver'){
            steps{
                  sh './scripts/deliver.sh'
            }
        }
    }
}